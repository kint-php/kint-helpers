{
    "name": "kint-php/kint-helpers",
    "description": "Kint helper functions",
    "keywords": ["kint", "php", "debug"],
    "type": "library",
    "homepage": "https://kint-php.github.io/kint/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Vollebregt",
            "homepage": "https://github.com/jnvsor"
        }

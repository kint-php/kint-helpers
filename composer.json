    "require": {
        "php": ">=7.1",
        "kint-php/kint": "^4 || ^5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "files": ["init.php"]
    },
    "scripts": {
        "format": "php-cs-fixer fix"
    }
}
